<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yaokun.movebusiness">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Movebusiness"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.AuthorizationActivity" />
        <activity android:name=".ui.ModPwdSucceedActivity" />
        <activity android:name=".ui.ChangePasswordActivity" />
        <activity android:name=".ui.AuthDetailsActivity"/>
        <activity
            android:name=".ui.VerificationCodeActivity"
            android:exported="false" />
        <activity
            android:name=".ui.RetrievePasswordActivity"
            android:exported="false" />
        <activity
            android:name=".ui.RetrievePassword2Activity"
            android:exported="false" />
        <activity
            android:name=".ui.MainActivity"
            android:exported="false" />
        <activity android:name=".ui.MineHomeActivity" />
        <activity android:name=".ui.RecordActivity" />
        <activity
            android:name=".ui.LoginActivity"
            android:exported="true"
            android:fitsSystemWindows="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>